            <string>var lienzo = surface_create(150, 200);
surface_set_target(lienzo);
draw_clear_alpha(c_black, 0);
var xx = 75;
var yy = 180;
var ac = altura / 85;
var yc = 30 * (1 - ac);

// pies
draw_sprite_ext(d_avatar, 1, xx, yy, 1, 1, 0, c_clan[clan], 1);
draw_sprite_ext(d_avatar, 2, xx, yy, 1, 1, 0, c_clan[clan], 1);

switch tipo {
    
    case m_tipo_user:
        // cuerpo
        draw_sprite_ext(d_avatar, 3 + body, xx, yy + yc, 1, 1, 0, c_clan[clan], ac);
        draw_sprite_ext(d_avatar, 29 + body * 5 + ropa, xx, yy + yc, 1, 1, 0, c_oscu[clan], ac);
        
        // cabeza
        draw_sprite_ext(d_avatar, 0, xx, yy + 85 - altura, 1, 1, 0, c_clan[clan], 1);
        draw_sprite_ext(d_avatar, 8 + cara, xx, yy + 85 - altura, 1, 1, 0, c_blan[clan], 1);
        draw_sprite_ext(d_avatar, 13 + pelo, xx, yy + 85 - altura, 1, 1, 0, c_oscu[clan], 1);
        
        // nombre
        draw_set_font(d_fontota);
        draw_set_valign(fa_bottom);
        draw_set_halign(fa_center);
        var alt = yy - 150 + 85 - altura;
        draw_text_transformed_colour(xx + 1, alt, name,
            0.9, 0.9, 0, c_white, c_white, c_white, c_white, 1);
        draw_text_transformed_colour(xx - 1, alt, name,
            0.9, 0.9, 0, c_white, c_white, c_white, c_white, 1);
        draw_text_transformed_colour(xx, alt, name,
            0.9, 0.9, 0, c_char[clan], c_char[clan], c_char[clan], c_char[clan], 1);
        draw_set_font(d_font);
        
        break;
    
    case m_tipo_elem:
        // cuerpo
        draw_sprite_ext(d_avatar, 3 + clan, xx, yy + yc, 1, 1, 0, c_clan[clan], ac);
        
        // cabeza
        draw_sprite_ext(d_avatar, 0, xx, yy + 85 - altura, 1, 1, 0, c_clan[clan], 1);
        draw_sprite_ext(d_avatar, 29 + clan, xx, yy + 85 - altura, 1, 1, 0, c_oscu[clan], 1);
        
        break;
    
    case m_tipo_zomb:
        // cuerpo
        draw_sprite_ext(d_avatar, 3, xx, yy + yc, 1, 1, 0, c_clan[clan], ac);
        
        // cabeza
        draw_sprite_ext(d_avatar, 0, xx, yy + 85 - altura, 1, 1, 0, c_clan[clan], 1);
        draw_sprite_ext(d_avatar, 54, xx, yy + 85 - altura, 1, 1, 0, c_oscu[clan], 1);
        
        break;
}

surface_reset_target();
draw_surface_ext(lienzo, x - xx, y - yy, 1, 1, 0, c_white, reloj_fin / m_descompone_s);
surface_free(lienzo);

