            <string>if altura &gt; 0 {
    altura = max(0, altura - m_gravedad * dlt);
    if altura == 0 {
        if name == "" {
            // borrarlo por zombi o elemental
            borrar = true;
        }
        else if pila_cadaveres &lt; war_max / 2 {
            // no borrarlo, aumenta la pila
            pila_cadaveres++;
        }
        else {
            // no borrarlo, borrar al mas viejito
            var ant = noone;
            var mini = o_control.tiempo_sim;
            with o_muerte {
                if altura == 0 and !borrar {
                    if marca_tmp &lt; mini {
                        mini = marca_tmp;
                        ant = id;
                    }
                }
            }
            if ant != noone {
                ant.borrar = true;
            }
        }
        // procurar distanciarse de otras cosas
        var f = 100;
        var vx, vy;
        while f &gt; 0 and (place_meeting(x, y, o_muerte) or
                place_meeting(x, y, o_bloque)) {
            f--;
            vx = x;
            vy = y;
            x += random_range(-6, 6);
            y += random_range(-6, 6);
            if s_mi_dir(x, y, mundo) == -1 {
                x = vx;
                y = vy;
            }
        }
        depth = -y;
    }
}
else if borrar and pila_craneos &gt; m_craneos_max {
    reloj_fin -= dlt;
    if reloj_fin &lt;= 0 {
        instance_destroy();
    }
}

</string>
