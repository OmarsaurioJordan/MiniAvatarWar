if keyboard_check_pressed(vk_anykey) {
switch keyboard_key {
    case vk_backspace:
        game_restart();
        break;
    case vk_escape:
        game_end();
        break;
    case vk_enter:
        var fff = get_save_filename("IMAGE|*.png", "img");
        if fff != "" {
            screen_save(fff);
        }
        break;
    case ord('1'):
        var aux = instance_nearest(mouse_x, mouse_y, o_avatar);
        aux.clan++; if aux.clan &gt; 4 { aux.clan = 0; }
        break;
    case ord('2'):
        var aux = instance_nearest(mouse_x, mouse_y, o_avatar);
        aux.cara++; if aux.cara &gt; 4 { aux.cara = 0; }
        break;
    case ord('3'):
        var aux = instance_nearest(mouse_x, mouse_y, o_avatar);
        aux.tool++; if aux.tool &gt; 4 { aux.tool = 0; }
        break;
    case ord('4'):
        var aux = instance_nearest(mouse_x, mouse_y, o_avatar);
        aux.arma++; if aux.arma &gt; 4 { aux.arma = 0; }
        break;
    case ord('5'):
        var aux = instance_nearest(mouse_x, mouse_y, o_avatar);
        aux.pelo++; if aux.pelo &gt; 4 { aux.pelo = 0; }
        break;
    case ord('6'):
        var aux = instance_nearest(mouse_x, mouse_y, o_avatar);
        aux.body++; if aux.body &gt; 4 { aux.body = 1; }
        break;
    case ord('7'):
        var aux = instance_nearest(mouse_x, mouse_y, o_avatar);
        aux.ropa++; if aux.ropa &gt; 4 { aux.ropa = 0; }
        break;
} }

